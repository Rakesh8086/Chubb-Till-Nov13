package com.app.payment;

import java.util.*;
import java.util.function.Function;
import java.util.stream.Collectors;

public class ComparatorSorting {
    public static void main(String[] args) {
        List<Accounts> accounts = Arrays.asList(
                new Accounts(101, "AAA", 500.00),
                new Accounts(102, "BBB", 600.00),
                new Accounts(103, "CCC", 700.00),
                new Accounts(104, "DDD", 800.00)
        );


        CustomComparator com = new CustomComparator();

        // Sort using the Stream API with our custom comparator
        List<Account> sortedAccounts = accounts.stream()
                .sorted(comparator)
                .collect(Collectors.toList());
        
        

    }
}