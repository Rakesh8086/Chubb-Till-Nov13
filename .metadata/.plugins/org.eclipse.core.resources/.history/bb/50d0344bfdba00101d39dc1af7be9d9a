package com.app.payment;

import java.util.Comparator;

public class CustomComparator implements Comparator<Accounts> {

    @Override
    public int compare(Accounts a1, Accounts a2) {
        return Integer.compare(a1.getAccountNo(), a2.getAccountNo());
    }
    
    @Override
    public int compare2(Accounts a1, Accounts a2) {
        return String.compare(a1.getName(), a2.getName());
    }
}
